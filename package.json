  },
  "license": "BSD-3-Clause",
  "readmeFilename": "README.md",
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "test": "cross-env NODE_PATH=./src/js/ ./node_modules/mocha/bin/mocha",
    "nyan": "cross-env NODE_PATH=./src/js/ ./node_modules/mocha/bin/mocha --reporter nyan",
    "test-nyc": "cross-env NODE_PATH=./src/js/ nyc ./node_modules/mocha/bin/mocha",
    "test-nyc-html": "cross-env NODE_PATH=./src/js/ nyc --reporter=html ./node_modules/mocha/bin/mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "4.1.2",
    "eslint": "^4.18.2",
    "jshint": "2.9.5",
    "jslint": "0.12.0",
    "mocha": "4.1.0",
    "stryker": "0.19.0",
    "stryker-api": "0.13.0",
    "stryker-babel-transpiler": "^0.3.1",
    "stryker-javascript-mutator": "^0.4.0",
    "stryker-mocha-framework": "0.8.2",
    "stryker-mocha-runner": "0.10.4",
    "webpack": "3.7.1"
  }
